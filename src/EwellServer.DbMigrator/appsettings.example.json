{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{Offset:zzz}][{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{Offset:zzz}][{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/EwellServer"
  },
  "OpenIddict": {
    "Applications": {
      "EwellServer_Web": {
        "ClientId": "EwellServer_Web",
        "ClientSecret": "1q2w3e*",
        "RootUrl": "https://localhost:44359"
      },
      "EwellServer_App": {
        "ClientId": "EwellServer_App",
        "RootUrl": "http://localhost:4200"
      },
      "EwellServer_BlazorServerTiered": {
        "ClientId": "EwellServer_BlazorServerTiered",
        "ClientSecret": "1q2w3e*",
        "RootUrl": "https://localhost:44314"
      },
      "EwellServer_Swagger": {
        "ClientId": "EwellServer_Swagger",
        "RootUrl": "https://localhost:44393"
      },
      "EwellServer_DApps": [
        {
          "ClientId": "EwellServer_DApp",
          "ClientSecret": "1q2w3e*"
        }
      ]
    }
  }
}